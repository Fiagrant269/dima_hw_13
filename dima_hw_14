task 1
let a = 'true';
let b = false;
let c = 17;
let d = undefined;
let e = null;
console.log(typeof a); // string
console.log(typeof b); // boolean
console.log(typeof c); // number
console.log(typeof d); // undefined
console.log(typeof e); // object\
task 2
  <!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <title>Вопрос пользователю</title>
</head>
<body>

<h1>JavaScript</h1>

<script>
    let name = prompt("Дима");
    console.log("Имя пользователя:", name);

    let age = prompt("25?");
    console.log("Возраст пользователя:", age);
</script>

</body>
</html>
